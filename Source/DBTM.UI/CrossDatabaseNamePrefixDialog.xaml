<Window x:Class="DBTM.UI.CrossDatabaseNamePrefixDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Cross Database Name Prefix for Compiled SQL" Height="135" Width="270" 
        WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" ShowInTaskbar="False">
    
    <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
       
        <TextBlock  Name="textBlock1" VerticalAlignment="Top" Text="Cross Database Name Prefix : " />
        <TextBox  Name="txtCrossDatabseNamePrefix" VerticalAlignment="Bottom" Width="150" Height="25" Margin="0,10,0,10"/>
        
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
        <Button  Name="btnOkay" VerticalAlignment="Bottom" Content="Ok" Width="100" Height="30" Click="btnOkay_Click" Margin="0,0,2.5,0" IsDefault="True" />
            
        </StackPanel>
    </StackPanel>
</Window>
